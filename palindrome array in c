#include <stdio.h>
void check(int arr[]){
  int i=0;
  int j=6;
  for(i,j;i<j;i++,j--){
      if(arr[i]==arr[j])
      continue;
      else
      {
          printf("not a palindrome array");
          break;
      }
  }
    if(i==j)
    printf("palindrome array");
    return;
}
int main()
{
   int arr[7]={1,2,3,4,3,2,1};
   check(arr);
   return 0;
}
